{
  "name": "scaffolds",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "engines": {
    "node": ">=24.0.0"
  },
  "main": "dist/index.js",
  "bin": {
    "scaffolds": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "typecheck": "tsgo --noEmit",
    "build": "rimraf dist && tsgo",
    "test": "pnpm typecheck && node --test \"test/**/*.test.ts\""
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0 --fix",
      "prettier --write"
    ],
    "src/**/*.{json,md,html,yml,yaml}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com:ryougifujino/scaffolds.git"
  },
  "keywords": [],
  "author": "ryougifujino",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ryougifujino/scaffolds/issues"
  },
  "packageManager": "pnpm@10.28.2",
  "dependencies": {
    "@inquirer/prompts": "^8.0.1",
    "tar": "^7.5.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@eslint/js": "^10.0.1",
    "@types/node": "24.10.12",
    "@typescript/native-preview": "7.0.0-dev.20260208.1",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^17.3.0",
    "lefthook": "^2.1.0",
    "lint-staged": "^16.2.7",
    "prettier": "3.8.1",
    "rimraf": "^6.1.2",
    "typescript-eslint": "^8.54.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "lefthook"
    ]
  }
}
